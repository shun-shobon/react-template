    "type": "git",
    "url": "git+https://github.com/shun-shobon/react-template.git"
  },
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT='tsconfig.config.json' webpack",
    "clean": "rimraf dist",
    "dev": "cross-env TS_NODE_PROJECT='tsconfig.config.json' webpack-dev-server",
    "eslint": "eslint --ignore-path .gitignore '**/*.{ts,tsx}'",
    "eslint:fix": "eslint --ignore-path .gitignore '**/*.{ts,tsx}' --fix",
    "stylelint": "stylelint --ignore-path .gitignore '**/*.{css,scss}'",
    "stylelint:fix": "stylelint --ignore-path .gitignore '**/*.{css,scss}' --fix"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --fix",
    "**/*.{css,scss}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
