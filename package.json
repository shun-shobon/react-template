{
  "name": "react-template",
  "version": "0.0.0",
  "description": "Reactのプロジェクトテンプレートです。",
  "homepage": "https://github.com/shun-shobon/react-template#readme",
  "bugs": "https://github.com/shun-shobon/react-template/issues",
  "license": "MIT",
  "author": "Shuntaro Nishizawa <me@shun.technology> (https://github.com/shun-shobon)",
  "repository": "https://github.com/shun-shobon/react-template.git",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "dev": "webpack serve",
    "lint": "run-p eslint stylelint",
    "lint:fix": "run-p eslint:fix stylelint:fix",
    "eslint": "eslint --ignore-path .gitignore '**/*.{ts,tsx}'",
    "eslint:fix": "eslint --ignore-path .gitignore '**/*.{ts,tsx}' --fix",
    "stylelint": "stylelint --ignore-path .gitignore '**/*.{css,scss}'",
    "stylelint:fix": "stylelint --ignore-path .gitignore '**/*.{css,scss}' --fix"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --fix",
    "**/*.{css,scss}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "core-js": "^3.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@approvers/stylelint-config-scss": "^1.0.0",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/fibers": "^3.1.0",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sass": "^1.16.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "private": true
}
